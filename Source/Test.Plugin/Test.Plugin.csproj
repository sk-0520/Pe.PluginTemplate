      <Private>false</Private>
      <ExcludeAssets>runtime</ExcludeAssets>
    </ProjectReference>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="..\..\Plugin.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Update="assembly-info.json">
